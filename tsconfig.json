{
    "compilerOptions": {
      "target": "ES2019",
      "module": "ESNext",
      "jsx": "react",
      "declaration": true,
      "outDir": "dist",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "moduleResolution": "node",
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
  }